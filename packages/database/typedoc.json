{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": ["src/index.ts"],
    "entryPointStrategy": "resolve",
    "out": "docs",
    "cleanOutputDir": true,

    "readme": "README.md",
    "name": "@reputo/database",
    "includeVersion": true,

    "exclude": [
        "**/*.test.ts",
        "**/__tests__/**",
        "**/*.spec.ts",
        "**/stories/**",
        "**/cli/**",
        "**/registry/*.gen.ts"
    ],
    "excludeExternals": true,
    "excludePrivate": true,
    "excludeProtected": false,

    "categorizeByGroup": true,
    "visibilityFilters": {
        "protected": true,
        "private": false,
        "inherited": true,
        "external": false
    },

    "githubPages": false,
    "plugin": ["typedoc-plugin-markdown"],
    "commentStyle": "jsdoc",
    "excludeNotDocumented": false,
    "excludeNotDocumentedKinds": [],
    "sort": ["source-order"],
    "kindSortOrder": [
        "Project",
        "Module",
        "Namespace",
        "Enum",
        "Class",
        "Interface",
        "TypeAlias",
        "Constructor",
        "Property",
        "Variable",
        "Function",
        "Accessor",
        "Method"
    ]
}
