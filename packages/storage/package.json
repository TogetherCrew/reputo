{
    "name": "@reputo/storage",
    "version": "0.0.0",
    "description": "Framework-agnostic S3 storage layer for the Reputo ecosystem.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "private": true,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "type": "module",
    "module": "dist/index.js",
    "scripts": {
        "check": "pnpm exec biome check --write",
        "format": "pnpm exec biome format --write",
        "lint": "pnpm exec biome lint --write",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "typecheck": "tsc --noEmit",
        "build": "tsc",
        "release": "semantic-release",
        "docs": "typedoc",
        "clean": "rm -rf coverage dist docs node_modules tsconfig.tsbuildinfo"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.713.0",
        "@aws-sdk/s3-request-presigner": "^3.713.0"
    },
    "devDependencies": {
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/commit-analyzer": "^13.0.1",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/release-notes-generator": "^14.0.3",
        "@types/node": "^22.17.0",
        "semantic-release": "^24.2.0",
        "semantic-release-commits-lint": "^1.1.0",
        "tsx": "^4.7.0",
        "typedoc": "^0.28.13",
        "typedoc-plugin-markdown": "^4.9.0",
        "typescript": "^5.9.2",
        "vitest": "^3.2.4"
    },
    "keywords": [
        "typescript",
        "storage",
        "s3"
    ],
    "files": [
        "dist",
        "!dist/**/*.test.*"
    ],
    "author": {
        "name": "Behzad Rabiei",
        "url": "https://github.com/Behzad-rabiei"
    },
    "license": "GPL-3.0",
    "packageManager": "pnpm@10.12.4"
}
